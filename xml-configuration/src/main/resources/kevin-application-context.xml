            </map>
        </property>
    </bean>

    <bean id="beanD" class="ru.bogdanium.xml_configuration.kevin.BeanD" />

    <bean id="beanC" class="ru.bogdanium.xml_configuration.kevin.BeanC" autowire="constructor" />

    <bean id="beanB" class="ru.bogdanium.xml_configuration.kevin.BeanB" />

    <bean id="beanA" class="ru.bogdanium.xml_configuration.kevin.BeanA" autowire="constructor" />

