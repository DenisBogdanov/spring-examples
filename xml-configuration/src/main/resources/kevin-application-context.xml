       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="bean" class="ru.bogdanium.xml_configuration.kevin.Bean">
        <property name="list">
            <list>
                <value>Bo</value>
                <value>Ro</value>
                <value>Mir</value>
            </list>
        </property>

        <property name="set">
            <set>
                <value>Bu</value>
                <value>Ra</value>
                <value>Ti</value>
